# Environnement d'exécution (development, production, test)\nNODE_ENV=development\n\n# Configuration du serveur\nPORT=5000\nHOST=localhost\n\n# Configuration MongoDB\nMONGO_URI=mongodb://localhost:27017/cyberthreat-atlas\n\n# Configuration JWT (authentification)\nJWT_SECRET=change-this-to-a-secure-secret-key\nJWT_EXPIRES_IN=24h\nBCRYPT_ROUNDS=10\n\n# Configuration CORS\nCORS_ORIGIN=http://localhost:3000\n\n# Niveau de logging (debug, info, warn, error)\nLOG_LEVEL=info\nLOG_FORMAT=dev\n\n# Clés API (services externes)\nVIRUSTOTAL_API_KEY=your-virustotal-api-key\nSHODAN_API_KEY=your-shodan-api-key\nALIENVAULT_API_KEY=your-alienvault-api-key\n\n# Configuration des tâches planifiées (cron)\nUPDATE_MITRE_CRON=0 0 * * * # Tous les jours à minuit\nFETCH_THREAT_INTEL_CRON=0 */6 * * * # Toutes les 6 heures